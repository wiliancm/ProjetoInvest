    function CarregaResumo(ativo, categoria) {
        if ($("#rentAtivo").text() == ativo) {
            $(".resumo").toggle();
        }
        else {
            $(".resumo").attr("style", "display: normal;");
        }

        $("#rentAtivo").text(ativo);

        var divMensalEsperado = divAnualEsperado = magicNumber = dy = cota = saldo = 0;

        cota = $("#impVlAtual" + ativo).val();
        dy = $("#impDY" + ativo).val() / 100;
        saldo = $("#impSaldo" + ativo).val();

        divMensalEsperado = (saldo * dy / 12).toFixed(2);
        divAnualEsperado = (saldo * dy).toFixed(2);
        magicNumber = (cota / (dy / 12) / cota).toFixed(0);

        $("#cotacaoAtual").text("R$ " + cota);
        $("#dyMensal").text((dy / 12 * 100) + "%");
        $("#dyAnual").text((dy * 100) + "%");

        $("#divMensalEsperado").text("R$ " + divMensalEsperado);
        $("#divAnualEsperado").text("R$ " + divAnualEsperado);
        $("#magicNumber").text(magicNumber);

        var scrollPos = $(".resumo").offset().top;
        $(window).scrollTop(scrollPos - 20);
    }

    function CarregaRender() {

        if ($("#carteira").val() === null || $("#carteira").val() === '') {
