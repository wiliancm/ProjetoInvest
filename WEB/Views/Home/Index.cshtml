@model bool
@{
    ViewData["Title"] = "Home Page";
}
<!-- CARREGANDO -->
<div id="carregando" style="display: none;">
    <img src="~/icons8-girador.gif" width="100px" />
</div>
<!-- CARREGANDO -->
<!-- LOGIN -->
<div id="inicio">
    <div class="box especial margin-top-30 return-investment" style="width: 450px;">
        <header>
            <span class="icon"><img style="padding: 0 0px" src="https://investidor10.com.br/assets/front/images/line-graph.svg" alt="Início"></span>
            <h2>
                início
            </h2>
        </header>
        <div id="info_about" class="content" style="align-content: center; text-align: center;">
            <div class="title">
                Insira seu ID de Carteira
            </div>
            <br />
            <input id="carteira" type="text" class="text-field" style="width: 300px; text-align: center;" />
            <p><span id="mensagemErro" style="color: red; display: none;">Favor informar um id de carteira.</span></p>
            <button onclick="javascript:CarregaRender();" class="btn-default black">
                Pesquisar
            </button>
            <br />
        </div>
    </div>
</div>
<!-- LOGIN -->
<!-- CONTEUDO -->
